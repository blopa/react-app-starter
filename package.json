{
    "name": "react-app-starter",
    "version": "0.0.1",
    "description": "A template for quickly setting up a react/redux/Material UI frontend",
    "keywords": [
        "Material UI",
        "React",
        "Redux"
    ],
    "repository": "github:blopa/react-app-starter",
    "main": "src/index.js",
    "scripts": {
        "build": "npm run clean && webpack --mode production --progress",
        "clean": "rm -rf build && mkdir build",
        "dev": "webpack-dev-server --mode development --progress",
        "dev:hot": "webpack-dev-server --mode development --progress --hot",
        "serve": "serve build",
        "setup": "rm -rf node_modules && touch .env && npm install && npm audit fix"
    },
    "author": "Pablo Pirata",
    "dependencies": {
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1",
        "clsx": "^1.1.1",
        "core-js": "^3.6.5",
        "dotenv-defaults": "^2.0.0",
        "dotenv-webpack": "^1.8.0",
        "prop-types": "^15.7.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-redux": "^7.2.0",
        "recompose": "^0.30.0",
        "redux": "^4.0.5",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.3.0",
        "typeface-roboto": "0.0.75"
    },
    "devDependencies": {
        "@babel/core": "^7.10.4",
        "@babel/plugin-proposal-class-properties": "^7.10.4",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-react-jsx": "^7.10.4",
        "@babel/plugin-transform-runtime": "^7.10.4",
        "@babel/preset-env": "^7.10.4",
        "@babel/preset-react": "^7.10.4",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.1.0",
        "babel-plugin-import": "^1.13.0",
        "brotli-webpack-plugin": "^1.1.0",
        "copy-webpack-plugin": "^6.0.3",
        "css-loader": "^3.6.0",
        "eslint": "^7.4.0",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-import-resolver-webpack": "^0.12.2",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-babel": "^5.3.1",
        "eslint-plugin-filenames": "^1.3.2",
        "eslint-plugin-flowtype": "^5.2.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jest": "^23.18.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.20.3",
        "eslint-plugin-react-hooks": "^4.0.5",
        "eslint-plugin-unicorn": "^20.1.0",
        "file-loader": "^6.0.0",
        "html-loader": "^1.1.0",
        "html-webpack-plugin": "^4.3.0",
        "json-loader": "^0.5.7",
        "less": "^3.11.3",
        "less-loader": "^6.2.0",
        "node-sass": "^4.14.1",
        "sass-loader": "^9.0.1",
        "serve": "^11.3.2",
        "style-loader": "^1.2.1",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0"
    }
}
